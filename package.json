{
  "private": true,
  "name": "patrick-hooper",
  "version": "3.0.0",
  "description": "The portfolio website for Patrick Hooper.",
  "homepage": "https://patrickhooper.me",
  "bugs": {
    "url": "https://github.com/pdhoopr/website/issues"
  },
  "license": "MIT",
  "author": "Patrick Hooper <patrick.d.hooper@gmail.com> (https://patrickhooper.me)",
  "repository": {
    "type": "git",
    "url": "https://github.com/pdhoopr/website"
  },
  "scripts": {
    "build": "npx --node-arg '-r esm' gatsby build",
    "check:css": "stylelint '**/*.js'",
    "check:format": "prettier --list-different '**/*.{js,json,md}'",
    "check:js": "eslint '**/*.js'",
    "develop": "npx --node-arg '-r esm' gatsby develop -H 0.0.0.0",
    "format": "prettier --write '**/*.{js,json,md}'",
    "serve": "npm run build && npx --node-arg '-r esm' gatsby serve -H 0.0.0.0",
    "start": "npm run develop"
  },
  "dependencies": {
    "@svgr/webpack": "^4.3.2",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.16",
    "gatsby-plugin-manifest": "^2.2.16",
    "gatsby-plugin-nprogress": "^2.1.6",
    "gatsby-plugin-offline": "^3.0.7",
    "gatsby-plugin-react-helmet": "^3.1.7",
    "gatsby-plugin-sitemap": "^2.2.12",
    "gatsby-plugin-styled-components": "^3.1.5",
    "gatsby-plugin-svgr": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "esm": "^3.2.25",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
