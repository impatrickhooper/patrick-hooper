{
  "name": "patrick-hooper",
  "version": "3.0.0",
  "description": "The portfolio website for Patrick Hooper.",
  "homepage": "https://patrickhooper.me",
  "bugs": {
    "url": "https://github.com/pdhoopr/website/issues"
  },
  "license": "MIT",
  "author": "Patrick Hooper <patrick.d.hooper@gmail.com> (https://patrickhooper.me)",
  "repository": {
    "type": "git",
    "url": "https://github.com/pdhoopr/website"
  },
  "scripts": {
    "build": "npx --node-arg '-r esm' gatsby build",
    "develop": "npx --node-arg '-r esm' gatsby develop -H 0.0.0.0",
    "format": "prettier --write '**/*.{js,json,md}'",
    "format:diff": "prettier --list-different '**/*.{js,json,md}'",
    "lint:css": "stylelint '**/*.js'",
    "lint:js": "eslint '**/*.js'",
    "serve": "npm run build && npx --node-arg '-r esm' gatsby serve -H 0.0.0.0",
    "start": "npm run develop"
  },
  "dependencies": {
    "@svgr/webpack": "^5.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.19.17",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-nprogress": "^2.1.19",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-plugin-svgr": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "esm": "^3.2.25",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "stylelint": "^13.1.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-processor-styled-components": "^1.9.0"
  },
  "private": true
}
